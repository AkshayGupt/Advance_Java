<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.com/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
 	http://www.springframework.org/schema/context
 	http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />
	<bean
		class="com.springcoretraining.autowiring.annotations.Address"
		name="address1">
		<property name="street" value="Bishtupur" />
		<property name="city" value="Kolkata" />
	</bean>
	<!-- <bean class="com.springcoretraining.autowiring.annotations.Address" 
		name="address2"> <property name="street" value="Salt Lake" /> <property name="city" 
		value="Kolkata" /> </bean> -->

	<bean
		class="com.springcoretraining.autowiring.annotations.Employee"
		name="employee_1" />
</beans>
